.PHONY : clean all

all:
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=genome $(MAKE) -f ../stamp.mk
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=intruder $(MAKE) -f ../stamp.mk
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=labyrinth $(MAKE) -f ../stamp.mk
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=kmeans $(MAKE) -f ../stamp.mk
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=ssca2 $(MAKE) -f ../stamp.mk
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=vacation $(MAKE) -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=genome $(MAKE) -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=intruder $(MAKE) -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=labyrinth $(MAKE) -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=kmeans $(MAKE) -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=ssca2 $(MAKE) -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=vacation $(MAKE) -f ../stamp.mk
	
clean:
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=genome $(MAKE) clean -f ../stamp.mk
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=intruder $(MAKE) clean -f ../stamp.mk
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=labyrinth $(MAKE) clean -f ../stamp.mk
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=kmeans $(MAKE) clean -f ../stamp.mk
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=ssca2 $(MAKE) clean -f ../stamp.mk
	API=lambda STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=vacation $(MAKE) clean -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=genome $(MAKE) clean -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=intruder $(MAKE) clean -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=labyrinth $(MAKE) clean -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=kmeans $(MAKE) clean -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=ssca2 $(MAKE) clean -f ../stamp.mk
	API=lambda_lite STAMP_FOLDER=stamp_lambda BENCH_ROOT=../ PROG=vacation $(MAKE) clean -f ../stamp.mk